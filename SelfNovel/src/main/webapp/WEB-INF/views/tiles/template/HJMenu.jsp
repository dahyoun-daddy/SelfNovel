<%@ page pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<style type="text/css">
@import url(http://fonts.googleapis.com/earlyaccess/hanna.css);

.nave, .nave ul {
	margin: 0;
	padding: 0;
	list-style: none;
	line-height: 1;
}


.nave {
	position: relative;
	margin: auto; /* Centering the menu */
	height: 50px;
	width: 900px;
	text-align: center;
	box-shadow: 0 1px 2px rgba(0, 0, 0, .3);
	background: #1a9de1;
	background-image: url("<c:url value='/resources/img/pattern.png'/>");
}

.nave ul {
	display: none;
}

.nave >li {
	margin: 0;
	line-height: 1;
	padding: 0;
	display: inline;
	position: relative;
	margin: 0 12px;
}
.nave::after, .nave::before {
    content: "";
    display: block;
    position: absolute;
    top: 6px;
    height: 0px;
    width: 0px;
    border: 23px solid #1a9de1;
    z-index: -1;
}
 
/* The left ribbon */
.nave::before {
    border-left-color: transparent;
    left: -30px;
}
 
/* The right ribbon */
.nave::after {
    border-right-color: transparent;
    right: -30px;
}

.nave>li>a {
    display: inline-block;
    padding: 15px 20px;
    position: relative;
 
    font-family: 'Hanna', serif;
    font-size: 20px;
    text-transform: uppercase;
    text-decoration: none;
    color: #fff;
    text-shadow: 1px 2px rgba(0, 0, 0, .2);
 
    -webkit-transition: color .3s linear;
       -moz-transition: color .3s linear;
         -o-transition: color .3s linear;
        -ms-transition: color .3s linear;
            transition: color .3s linear;
}
 
.nave>li>a:hover, .nav>li:hover>a {
    color: #FFE400;
}

.nave>li>a::after {
    content: "";
    height: 15px;
    width: 15px;
    background-image: url(data:image/png;base64,);
    position: absolute;
    right: -20px;
    top: 16px;
    display: block;
}

.nave>li:last-child>a::after {
    display: none;
}


</style>
